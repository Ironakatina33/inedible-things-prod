<div id="<%= dom_id article %>">
  <p>
    <strong>Title:</strong>
    <%= article.title %>
  </p>

  <p>
    <strong>Content:</strong>
    <%= article.content %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= article.image %>
  </p>

  <p>
  <strong>Prompt:</strong>
  <%= article.prompt %>
</p>

<p>
  <strong>Result:</strong>
  <%= article.result %>
</p>

</div>
